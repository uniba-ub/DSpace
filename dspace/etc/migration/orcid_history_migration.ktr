    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>INSERT INTO metadatavalue 
SELECT 
nextval('metadatavalue_seq'),
(SELECT field.metadata_field_id 
 from metadatafieldregistry field, metadataschemaregistry sc 
 where field.element = 'orcid' 
 and field.qualifier = 'authenticated'
 and sc.short_id = 'dspace'
 and field.metadata_schema_id = sc.metadata_schema_id),
'true',
null,
0,
null,
-1,
item.uuid,
null
FROM item item 
WHERE exists ( 
	SELECT * FROM metadatavalue 
	where dspace_object_id = item.uuid
	and metadata_field_id = (SELECT field.metadata_field_id 
							 from metadatafieldregistry field, metadataschemaregistry sc 
							 where field.element = 'orcid' 
							 and field.qualifier = 'access-token'
							 and sc.short_id = 'cris' 
							 and field.metadata_schema_id = sc.metadata_schema_id)
) and exists ( 
	SELECT * FROM metadatavalue 
	where dspace_object_id = item.uuid
	and metadata_field_id = (SELECT field.metadata_field_id 
							 from metadatafieldregistry field, metadataschemaregistry sc 
							 where field.element = 'identifier' 
							 and field.qualifier = 'orcid'
							 and sc.short_id = 'person' 
							 and field.metadata_schema_id = sc.metadata_schema_id)
);</sql>
